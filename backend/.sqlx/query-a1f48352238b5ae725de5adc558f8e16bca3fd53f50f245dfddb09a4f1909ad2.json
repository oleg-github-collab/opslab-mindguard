  "query": "\n        SELECT k.id, k.from_user_id, k.to_user_id, k.message, k.created_at,\n               u.enc_name as \"from_user_enc_name!\"\n        FROM kudos k\n        JOIN users u ON k.from_user_id = u.id\n        WHERE k.to_user_id = $1\n        ORDER BY k.created_at DESC\n        LIMIT $2\n        ",
